package com.code.entity.log;

import com.code.entity.CommenEntity;

import javax.persistence.Entity;
import javax.persistence.Table;

/**
 * 日志模板
 * @author Jinx
 *
 */
@Entity
@Table(name="web_log_template")
public class LogTemplate extends CommenEntity{

	private static final long serialVersionUID = 1829867254766999989L;

	//日志编码
	private String code;
	//日志模板
	private String content;
	//是否启用
	private Integer status;
	//备注
	private String remark;
	//用途1 短信 2 邮件 3 微信 4 网站站内信 5为存储在数据库文件
	private Integer type;
	
	
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	
}
