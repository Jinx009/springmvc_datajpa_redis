package com.code.entity.account;

import com.code.entity.CommenEntity;
import com.code.entity.user.User;

import javax.persistence.*;

/**
 * 资金日志
 * @author Jinx
 *
 */
@Entity
@Table(name = "web_account_log")
public class AccountLog extends CommenEntity{

	private static final long serialVersionUID = 6444699026225953398L;

	//支付方式-网银直联
	private static final String TYPE_ONLINE = "网银直联";
	//用户
	@ManyToOne(fetch = FetchType.LAZY)
	@JoinColumn(name = "user_id")
	private User user;
	//类型 产生这条记录的操作
	private String type;
	//操作金额
	private Double money;
	//可用金额
	private Double useMoney;
	//投资金额
	private Double investMoney;
	//备注
	private String remark;
	//添加ip
	private String addIp;
	//收支明细 0是不变  1是收入  2是支出
	private byte paymentsType;
	
	
	
	
	public User getUser() {
		return user;
	}
	public void setUser(User user) {
		this.user = user;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public Double getMoney() {
		return money;
	}
	public void setMoney(Double money) {
		this.money = money;
	}
	public Double getUseMoney() {
		return useMoney;
	}
	public void setUseMoney(Double useMoney) {
		this.useMoney = useMoney;
	}
	public Double getInvestMoney() {
		return investMoney;
	}
	public void setInvestMoney(Double investMoney) {
		this.investMoney = investMoney;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public String getAddIp() {
		return addIp;
	}
	public void setAddIp(String addIp) {
		this.addIp = addIp;
	}
	public byte getPaymentsType() {
		return paymentsType;
	}
	public void setPaymentsType(byte paymentsType) {
		this.paymentsType = paymentsType;
	}
	public static String getTypeOnline() {
		return TYPE_ONLINE;
	}
}
