package com.code.entity.pro;

import com.code.entity.CommenEntity;

import javax.persistence.*;
import java.util.Date;

/**
 * 项目
 * @author Jinx
 *
 */
@Entity
@Table(name="web_project")
public class Project extends CommenEntity{

	private static final long serialVersionUID = 7978348468985112741L;
	
	//项目名称
	private String projectName;
	//项目类型 1 实物众筹 2股权众筹 4公益众筹
	private Integer type;
	//违约金比例
	private Double breach;
	//项目简介
	private String info;
	//单笔最小投资金额
	private Double minMoney;
	//单笔最大支持金额
	private Double maxMoney;
	//最大接受人数
	private Integer maxInvestor;
	//金额增加规则
	private Double addAmount;
	//募集金额
	private Double wannaAccount;
	//已募集金额
	private Double account;
	//开始时间
	@Temporal(TemporalType.DATE)
	@Column(name = "start_time")
	private Date startTime;
	//结束时间
	@Temporal(TemporalType.DATE)
	@Column(name = "end_time")
	private Date endTime;
	//融资伦次
	private Integer financing;
	//发起人
	private String creater;
	//发起人公司
	private String company;
	//发起人简介
	private String createrInfo;
	//PC是否显示0代表否 1代表是
	private Integer isShowPc;
	//微信是否显示
	private Integer isShowWechat;
	//app是否显示
	private Integer isShowApp;
	//是否接受超人数
	private Integer isExceedAcceptInvestor;
	//是否接受超募
	private Integer isExceedAccept;
	//定向码
	private String acceptanceCode;
	//是否首页推荐
	private Integer isRecommend;
	//推荐序号
	private Integer recommendNum;
	//项目状态 -1 待初审 0初审中 1 初审驳回 2 众筹中 3 众筹完成 4 过期 5撤回  
	private Integer status;
	 
	 
	 
	 
	 
	 
	public String getProjectName() {
		return projectName;
	}
	public void setProjectName(String projectName) {
		this.projectName = projectName;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	public Double getBreach() {
		return breach;
	}
	public void setBreach(Double breach) {
		this.breach = breach;
	}
	public String getInfo() {
		return info;
	}
	public void setInfo(String info) {
		this.info = info;
	}
	public Double getMinMoney() {
		return minMoney;
	}
	public void setMinMoney(Double minMoney) {
		this.minMoney = minMoney;
	}
	public Double getMaxMoney() {
		return maxMoney;
	}
	public void setMaxMoney(Double maxMoney) {
		this.maxMoney = maxMoney;
	}
	public Double getAddAmount() {
		return addAmount;
	}
	public void setAddAmount(Double addAmount) {
		this.addAmount = addAmount;
	}
	public Double getWannaAccount() {
		return wannaAccount;
	}
	public void setWannaAccount(Double wannaAccount) {
		this.wannaAccount = wannaAccount;
	}
	public Double getAccount() {
		return account;
	}
	public void setAccount(Double account) {
		this.account = account;
	}
	public Date getStartTime() {
		return startTime;
	}
	public void setStartTime(Date startTime) {
		this.startTime = startTime;
	}
	public Date getEndTime() {
		return endTime;
	}
	public void setEndTime(Date endTime) {
		this.endTime = endTime;
	}
	public Integer getFinancing() {
		return financing;
	}
	public void setFinancing(Integer financing) {
		this.financing = financing;
	}
	public String getCreater() {
		return creater;
	}
	public void setCreater(String creater) {
		this.creater = creater;
	}
	public String getCompany() {
		return company;
	}
	public void setCompany(String company) {
		this.company = company;
	}
	public String getCreaterInfo() {
		return createrInfo;
	}
	public void setCreaterInfo(String createrInfo) {
		this.createrInfo = createrInfo;
	}
	public Integer getIsShowPc() {
		return isShowPc;
	}
	public void setIsShowPc(Integer isShowPc) {
		this.isShowPc = isShowPc;
	}
	public Integer getIsShowWechat() {
		return isShowWechat;
	}
	public void setIsShowWechat(Integer isShowWechat) {
		this.isShowWechat = isShowWechat;
	}
	public Integer getIsShowApp() {
		return isShowApp;
	}
	public void setIsShowApp(Integer isShowApp) {
		this.isShowApp = isShowApp;
	}
	public Integer getIsExceedAcceptInvestor() {
		return isExceedAcceptInvestor;
	}
	public void setIsExceedAcceptInvestor(Integer isExceedAcceptInvestor) {
		this.isExceedAcceptInvestor = isExceedAcceptInvestor;
	}
	public Integer getIsExceedAccept() {
		return isExceedAccept;
	}
	public void setIsExceedAccept(Integer isExceedAccept) {
		this.isExceedAccept = isExceedAccept;
	}
	public String getAcceptanceCode() {
		return acceptanceCode;
	}
	public void setAcceptanceCode(String acceptanceCode) {
		this.acceptanceCode = acceptanceCode;
	}
	public Integer getIsRecommend() {
		return isRecommend;
	}
	public void setIsRecommend(Integer isRecommend) {
		this.isRecommend = isRecommend;
	}
	public Integer getRecommendNum() {
		return recommendNum;
	}
	public void setRecommendNum(Integer recommendNum) {
		this.recommendNum = recommendNum;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public Integer getMaxInvestor() {
		return maxInvestor;
	}
	public void setMaxInvestor(Integer maxInvestor) {
		this.maxInvestor = maxInvestor;
	}
	
}
