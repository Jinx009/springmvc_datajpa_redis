package com.code.entity.pro;

import com.code.entity.CommenEntity;

import javax.persistence.Entity;
import javax.persistence.Table;

/**
 * 素材编码库
 * @author Jinx
 *
 */
@Entity
@Table(name="web_material_code")
public class MaterialCode extends CommenEntity{

	private static final long serialVersionUID = -6670826966555757110L;

	//素材名称
	private String materialName;
	//素材编码
	private String materialCode;
	//素材类型1.图片 2.文字 3.音频 4.视频 5.图文
	private Integer materialType;
	//是否显示
	private Integer isShow;
	
	
	
	
	
	
	public String getMaterialName() {
		return materialName;
	}
	public void setMaterialName(String materialName) {
		this.materialName = materialName;
	}
	public String getMaterialCode() {
		return materialCode;
	}
	public void setMaterialCode(String materialCode) {
		this.materialCode = materialCode;
	}
	public Integer getMaterialType() {
		return materialType;
	}
	public void setMaterialType(Integer materialType) {
		this.materialType = materialType;
	}
	public Integer getIsShow() {
		return isShow;
	}
	public void setIsShow(Integer isShow) {
		this.isShow = isShow;
	}
}
