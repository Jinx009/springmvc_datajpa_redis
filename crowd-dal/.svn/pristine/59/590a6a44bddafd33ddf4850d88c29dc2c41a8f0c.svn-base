package com.code.entity.pro;

import com.code.entity.CommenEntity;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name="web_flag")
public class Flag extends CommenEntity{

	private static final long serialVersionUID = 7673918321740675475L;
	
	//标签名称
	private String name;
	//是否启用
	private Integer isShow;
	
	
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Integer getIsShow() {
		return isShow;
	}
	public void setIsShow(Integer isShow) {
		this.isShow = isShow;
	}
}
