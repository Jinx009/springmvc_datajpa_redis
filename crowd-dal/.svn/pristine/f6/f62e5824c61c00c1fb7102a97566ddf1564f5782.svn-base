package com.code.entity.home;

import com.code.entity.CommenEntity;

import javax.persistence.Entity;
import javax.persistence.JoinColumn;
import javax.persistence.OneToOne;
import javax.persistence.Table;

/**
 * 后台用户导航权限
 * @author Jinx
 *
 */
@Entity
@Table(name="home_nav_list")
public class AdminNav extends CommenEntity{

	private static final long serialVersionUID = -5870131992600264840L;

	//对应导航
	@OneToOne
	@JoinColumn(name = "nav_id")
	private Nav nav;
	//后台用户
	@OneToOne
	@JoinColumn(name = "admin_id")
	private Admin admin;
	
	
	
	public Nav getNav() {
		return nav;
	}
	public void setNav(Nav nav) {
		this.nav = nav;
	}
	public Admin getAdmin() {
		return admin;
	}
	public void setAdmin(Admin admin) {
		this.admin = admin;
	}
}
