package com.code.entity.webuser;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Temporal;
import javax.persistence.TemporalType;

import com.code.entity.CommenEntity;

/**
 * 
 * @author 高雄辉
 * @description 第三方用户表
 *
 */
public class WebThridPartUser extends CommenEntity {
	
	/**
	 * 第三方来源
	 * 1:微信
	 * 2:微博
	 * 3:QQ
	 * 4:支付宝
	 * 5:邮箱
	 * 
	 */
	private int thridPartType;
	/**
	 * 第三方平台appid
	 */
	private String thridPartAppid;
	/**
	 * 第三方客户标志id
	 */
	private String thridPartOpenid;
	/**
	 * 登录次数
	 */
	private long loginTimes;
	/**
	 * 绑定真实账户id
	 */
	private long bindId;
	/**
	 * 绑定状态
	 * 0:已取消
	 * 1:绑定中
	 */
	private int bindStatus;
	/**
	 * 绑定/解绑时间
	 */
	@Column(name="bind_time")
	@Temporal(TemporalType.TIMESTAMP)
	private Date bindTime;
	/**
	 * 第三方用户昵称
	 */
	private String nickName;
	/**
	 * 第三方用户头像
	 */
	private String headPicPath;
	public int getThridPartType() {
		return thridPartType;
	}
	public void setThridPartType(int thridPartType) {
		this.thridPartType = thridPartType;
	}
	public String getThridPartAppid() {
		return thridPartAppid;
	}
	public void setThridPartAppid(String thridPartAppid) {
		this.thridPartAppid = thridPartAppid;
	}
	public String getThridPartOpenid() {
		return thridPartOpenid;
	}
	public void setThridPartOpenid(String thridPartOpenid) {
		this.thridPartOpenid = thridPartOpenid;
	}
	public long getLoginTimes() {
		return loginTimes;
	}
	public void setLoginTimes(long loginTimes) {
		this.loginTimes = loginTimes;
	}
	public long getBindId() {
		return bindId;
	}
	public void setBindId(long bindId) {
		this.bindId = bindId;
	}
	public int getBindStatus() {
		return bindStatus;
	}
	public void setBindStatus(int bindStatus) {
		this.bindStatus = bindStatus;
	}
	public Date getBindTime() {
		return bindTime;
	}
	public void setBindTime(Date bindTime) {
		this.bindTime = bindTime;
	}
	public String getNickName() {
		return nickName;
	}
	public void setNickName(String nickName) {
		this.nickName = nickName;
	}
	public String getHeadPicPath() {
		return headPicPath;
	}
	public void setHeadPicPath(String headPicPath) {
		this.headPicPath = headPicPath;
	}
	
	
}
