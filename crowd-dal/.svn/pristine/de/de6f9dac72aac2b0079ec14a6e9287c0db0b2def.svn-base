package com.code.entity.order;

import com.code.entity.CommenEntity;
import com.code.entity.pro.InvestRule;
import com.code.entity.pro.Project;
import com.code.entity.user.Address;
import com.code.entity.user.User;

import javax.persistence.Entity;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.Table;

@Entity
@Table(name="web_order")
public class Order extends CommenEntity{

	private static final long serialVersionUID = -3603492829076071691L;

	@ManyToOne
	@JoinColumn(name = "project_id")
	private Project project;
	
	@ManyToOne
	@JoinColumn(name = "user_id")
	private User user;
	
	@ManyToOne
	@JoinColumn(name = "invest_rule_id")
	private InvestRule investRule;
	
	//购买状态
	private Integer payStatus;
	
	@ManyToOne
	@JoinColumn(name = "address_id")
	private Address address;

	public Project getProject() {
		return project;
	}
	public void setProject(Project project) {
		this.project = project;
	}
	public User getUser() {
		return user;
	}
	public void setUser(User user) {
		this.user = user;
	}
	public InvestRule getInvestRule() {
		return investRule;
	}
	public void setInvestRule(InvestRule investRule) {
		this.investRule = investRule;
	}
	public Integer getPayStatus() {
		return payStatus;
	}
	public void setPayStatus(Integer payStatus) {
		this.payStatus = payStatus;
	}
	public Address getAddress() {
		return address;
	}
	public void setAddress(Address address) {
		this.address = address;
	}
}
