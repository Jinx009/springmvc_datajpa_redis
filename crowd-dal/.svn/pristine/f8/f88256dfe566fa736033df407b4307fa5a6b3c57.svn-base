package com.code.entity.home;

import com.code.entity.CommenEntity;

import javax.persistence.Entity;
import javax.persistence.JoinColumn;
import javax.persistence.OneToOne;
import javax.persistence.Table;

/**
 * 后台用户信息
 * @author Jinx
 *
 */
@Entity
@Table(name="home_userinfo")
public class AdminInfo extends CommenEntity{

	private static final long serialVersionUID = 6297136987285383384L;
	
	//邮箱
	private String email;
	//电话
	private String mobilePhone;
	//地址
	private String address;
	//身份证正面
	private String idCardA;
	//身份证背面
	private String idCradB;
	//营业执照
	private String companyPic;
	//账号信息
	@OneToOne
	@JoinColumn(name = "admin_id")
	private Admin admin;
	
	
	
	
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public String getMobilePhone() {
		return mobilePhone;
	}
	public void setMobilePhone(String mobilePhone) {
		this.mobilePhone = mobilePhone;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	public String getIdCardA() {
		return idCardA;
	}
	public void setIdCardA(String idCardA) {
		this.idCardA = idCardA;
	}
	public String getIdCradB() {
		return idCradB;
	}
	public void setIdCradB(String idCradB) {
		this.idCradB = idCradB;
	}
	public String getCompanyPic() {
		return companyPic;
	}
	public void setCompanyPic(String companyPic) {
		this.companyPic = companyPic;
	}
	public Admin getAdmin() {
		return admin;
	}
	public void setAdmin(Admin admin) {
		this.admin = admin;
	}
	
}
