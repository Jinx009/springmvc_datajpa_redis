<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/cxf/*</pattern>
		<pattern>/api/*</pattern>
		<pattern>/javasimon/*</pattern>
	</excludes>

	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
	<decorator name="manager" page="manager.jsp">
		<pattern>/manager/*</pattern>
	</decorator>
	<decorator name="pro" page="pro.jsp">
		<pattern>/pro/*</pattern>
	</decorator>
	<decorator name="finance" page="finance.jsp">
		<pattern>/finance/*</pattern>
	</decorator>
</decorators>
